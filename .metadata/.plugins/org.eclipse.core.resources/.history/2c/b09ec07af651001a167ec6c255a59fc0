package hello;

public class Variable {

	public static void main(String[] args) {
		byte b = 1;
		short s = 200;
		int i = 300;
		long l = 400;
		
//		byte b2 = 200;
		
		char c = '中';
//		char c2 = '中国';
//		char c3 = 'ab';
		
		double d = 123.45;
//		默认小数是double类型
//		float f = 54.321;
		float f2 = 54.321f;
		
		boolean b1 = true;
		boolean b2 = false;
//		boolean b3 = 1;
		
		String str = "hello world！";
		
		long val = 26L;
		int decVal = 26;
		int hexVal = 0x1a;
		int oxVal = 032;
		int binVal = 0b11010;
		System.out.println(hexVal);
	}
}
