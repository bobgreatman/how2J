package hello;

public class Operator {
	public static void main(String[] args) {
		int i = 10;
		int j = 5;
		int a = i + j;
		int c = i * j;
		int d = i / j;
		
		int a1 = 5;
		long b1 = 6;
		int c1 = (int)(a1 + b1);
		long d1 = a1 + b1;
		
		byte a2 = 1;
		byte b2 = 2;
		byte c2 = (byte)(a2 + b2);
		int d2 = a2 + b2;
		
		int i = 5;
		int j = 2;
		System.out.println(i % j);
	}
}
